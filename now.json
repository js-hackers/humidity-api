{
  "version": 2,
  "name": "weather-api",
  "alias": "weather-api.js-hackers.now.sh",
  "scope": "js-hackers",
  "env": {
    "OPENWEATHER_API_KEY": "@openweather-api-key"
  },
  "routes": [
    {"src": "/humidity", "dest": "/api/humidity"},
    {"src": "/random", "dest": "/api/random"},
    {"src": "/(.*)", "status": 404, "dest": "/404.html"}
  ]
}
