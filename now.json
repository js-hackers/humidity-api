{
  "version": 2,
  "name": "weather-api",
  "alias": "weather-api.js-hackers.now.sh",
  "scope": "js-hackers",
  "env": {
    "OPENWEATHER_API_KEY": "@openweather-api-key"
  },
  "rewrites": [
    {"source": "/humidity", "destination": "/api/humidity"},
    {"source": "/random", "destination": "/api/random"},
    {"source": "/(.*)", "status": 404, "destination": "/404.html"}
  ]
}
